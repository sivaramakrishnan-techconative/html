{
  "name": "@applitools/spec-driver-selenium",
  "version": "1.5.68",
  "keywords": [
    "selenium",
    "selenium-webdriver",
    "webdriver",
    "webdriverjs",
    "applitools",
    "automation",
    "testing",
    "tests"
  ],
  "homepage": "https://applitools.com",
  "bugs": {
    "url": "https://github.com/applitools/eyes.sdk.javascript1/issues"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/applitools/eyes.sdk.javascript1.git",
    "directory": "js/packages/spec-driver-selenium"
  },
  "license": "SEE LICENSE IN LICENSE",
  "author": {
    "name": "Applitools Team",
    "email": "team@applitools.com"
  },
  "exports": {
    ".": {
      "types": "./types/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "main": "./dist/index.js",
  "types": "./types/index.d.ts",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "lint": "run --top-level eslint '**/*.ts'",
    "build": "run --top-level tspc --project ./tsconfig.build.json",
    "test": "run --top-level mocha './test/**/*.spec.ts' --exit",
    "setup": "run --top-level browsers:setup",
    "up:framework": "echo \"$(jq '.devDependencies[\"selenium-webdriver\"] = $ENV.APPLITOOLS_FRAMEWORK_VERSION' ./package.json)\" > ./package.json"
  },
  "dependencies": {
    "@applitools/driver": "1.16.1",
    "@applitools/utils": "1.7.0"
  },
  "devDependencies": {
    "@applitools/api-extractor": "^1.2.22",
    "@applitools/test-utils": "^1.5.17",
    "@types/node": "^12.20.55",
    "@types/selenium-webdriver": "^4.1.15",
    "nock": "^13.3.2",
    "selenium-webdriver": "^4.15.0"
  },
  "peerDependencies": {
    "selenium-webdriver": ">=3.6.0"
  },
  "engines": {
    "node": ">=12.13.0"
  },
  "publishConfig": {
    "access": "public"
  }
}